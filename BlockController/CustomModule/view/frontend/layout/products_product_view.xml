<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">BlockController_CustomModule::images/logo.png</argument>
            </arguments>
        </referenceBlock>
    <referenceContainer name="content">
        <block class="BlockController\CustomModule\Block\Product\ProductList" name="phpcuong.product.list" template="BlockController_CustomModule::view/productList.phtml" />
    </referenceContainer>
</page>
